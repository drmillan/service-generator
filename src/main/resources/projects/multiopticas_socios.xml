<!-- ################################################################ XML 
	DE DEFINICIÓN DE SERVICIOS MULTIOPTICAS_SOCIOS ################################################################ -->

<protocol>
	<messages>
		<!-- ###################################### LOGIN ###################################### -->
		<message name="Login" service="MO" method="login" description=""
			type="Post">
			<urlPattern>
				<url address="http://${kLoginURL}" />
			</urlPattern>
			<request>
				<field name="activationCode" type="String" description="Codigo de activacion" />
			</request>
			<response>
				<field name="status" type="String" description="OK, NO_OK" />
				<field name="statusMessage" type="String" description="Descripción del error en caso de error" />
				<field name="appId" type="String" description="Id de aplicación" />
				<field name="memberId" type="String" description="Id de socio" />
			</response>
		</message>
		<!-- ###################################### REMOVE ORDER ###################################### -->
		<message name="RemoveOrder" service="MO" method="removeOrder"
			description="" type="Delete">
			<urlPattern>
				<url address="http://${kRemoveOrderURL}" />
			</urlPattern>
			<request>
				<field name="appId" type="String" description="Id de aplicación" />
				<field name="memberId" type="String" description="Id de socio" />
				<field name="orderId" type="int" description="Id del pedido" />
			</request>
			<response>
				<field name="status" type="String" description="OK, NO_OK" />
				<field name="statusMessage" type="String" description="Descripción del error en caso de error" />
			</response>
		</message>
		<!-- ###################################### ORDER HISTORY ############################## -->
		<message name="OrderHistory" service="MO" method="orderHistory"
			description="" type="Get">
			<urlPattern>
				<url address="http://${kOrderHistoryURL}" />
			</urlPattern>
			<request>
				<field name="appId" type="String" description="Id de aplicación" />
				<field name="memberId" type="String" description="Id de miembro" />
				<field name="fromDate" type="String" description="Fecha desde (dd/MM/yyyy)" />
				<field name="toDate" type="String" description="Fecha hasta (dd/MM/yyyy)" />
				<field name="name" type="String" description="Nombre de filtrado" />
				<field name="email" type="String" description="Email de filtrado" />
				<field name="phoneNumber" type="String" description="Número de teléfono de filtrado" />
			</request>
			<response>
				<field name="status" type="String" description="OK, NO_OK" />
				<field name="statusMessage" type="String" description="Descripción del error en caso de error" />
				<field name="orders" type="OrderDTO*" description="Pedidos" />
			</response>
		</message>
		<!-- ###################################### PRODUCT ############################## -->
		<message name="UpdateDB" service="MO" method="updateDB"
			description="" type="Get">
			<urlPattern>
				<url address="http://${kUpdateURL}" />
			</urlPattern>
			<request>
				<field name="appId" type="String" description="Id de aplicación" />
				<field name="memberId" type="String" description="Id de miembro" />
			</request>
			<response>
				<field name="status" type="String" description="OK/NO_OK" />
				<field name="statusMessage" type="String" description="Mensaje a mostrar al usuario en caso de error" />
				<field name="promos" type="PromoDTO*" description="Listado de campañas" />				
				<field name="ages" type="AgeDTO*" description="Listado de edades" />
				<field name="genders" type="GenderDTO*" description="Listado de generos" />
				<field name="frameTypes" type="FrameTypeDTO*" description="Listado de tipos de monturas" />
				<field name="colors" type="ColorDTO*" description="Listado de colores" />
				<field name="collections" type="CollectionDTO*" description="Listado de colecciones" />
				<field name="materials" type="MaterialDTO*" description="Listado de materiales" />
				<field name="frameSizes" type="FrameSizeDTO*" description="Listado de calibres" />
				<field name="framesFilters" type="FramesFilterDTO*" description="Listado de filtros para las monturas" />
				<field name="frames" type="FrameDTO*" description="Listado de monturas" />
				<field name="lensTypes" type="LensTypeDTO*" description="Listado de tipos de lentes" />
				<field name="warranties" type="WarrantyDTO*" description="Lista de garantías" />
			</response>
		</message>
		<!-- ###################################### SAVE ORDER ###################################### -->
		<message name="SaveOrder" service="MO" method="saveOrder"
			description="" type="Post">
			<urlPattern>
				<url address="http://${kSaveOrderURL}" />
			</urlPattern>
			<request>
				<field name="appId" type="String" description="Id de aplicación" />
				<field name="memberId" type="String" description="Id de socio" />
				<field name="orderId_MO" type="int" description="Id del pedido" />
				<field name="name" type="String" description="Nombre del cliente" />
				<field name="email" type="String" description="Email del cliente" />
				<field name="phoneNumber" type="String" description="Telefono del cliente" />
				<field name="comments" type="String" description="Observaciones" />
				<field name="date" type="long" description="Fecha" />
				<field name="promoId_MO" type="int" description="Id de la promocion" />
				<field name="promoName" type="String" description="Nombre de la promocion" />
				<field name="frameId_MO" type="int" description="Id de la montura" />
				<field name="frameName" type="String" description="Nombre de la montura" />
				<field name="framePrice" type="double" description="Precio de la montura" />
				<field name="variantId_MO" type="String" description="Id de la variante" />
				<field name="variantName" type="String" description="Nombre de la variante" />
				<field name="variantPrice" type="double" description="Precio de la variante" />
				<field name="lensTypeId_MO" type="int" description="Id del tipo de lente" />
				<field name="lensTypeName" type="String" description="Nombre del tipo de lente" />
				<field name="lensTypePrice" type="double" description="Precio del tipo de lente" />
				<field name="lensEquipmentId_MO" type="int" description="Id del equipamiento de la lente" />
				<field name="lensEquipmentName" type="String" description="Nombre del equipamiento de la lente" />
				<field name="lensEquipmentPrice" type="double" description="Precio del equipamiento de la lente" />
				<field name="treatmentOneId_MO" type="int" description="Id del tratamiento" />
				<field name="treatmentOneName" type="String" description="Nombre del tratamiento" />
				<field name="treatmentOnePrice" type="double" description="Precio del tratamiento" />
				<field name="treatmentTwoId_MO" type="int" description="Id del tratamiento" />
				<field name="treatmentTwoName" type="String" description="Nombre del tratamiento" />
				<field name="treatmentTwoPrice" type="double" description="Precio del tratamiento" />
				<field name="treatmentSunId_MO" type="int" description="Id del tratamiento" />
				<field name="treatmentSunName" type="String" description="Nombre del tratamiento" />
				<field name="treatmentSunPrice" type="double" description="Precio del tratamiento" />
				<field name="crossSellId_MO" type="int" description="Id de la venta cruzada" />
				<field name="crossSellName" type="String" description="Nombre de la venta cruzada" />
				<field name="crossSellPrice" type="double" description="Precio de la venta cruzada" />
				<field name="crossSellDetailId_MO" type="int" description="Id del detalle de la venta cruzada" />
				<field name="crossSellDetailName" type="String" description="Nombre del detalle de la venta cruzada" />
				<field name="crossSellDetailPrice" type="double" description="Precio del detalle de la venta cruzada" />
				<field name="warrantyId_MO" type="int" description="Id de la garantia" />
				<field name="warrantyName" type="String" description="Nombre de la garantia" />
				<field name="warrantyPrice" type="double" description="Precio de la garantia" />
				<field name="secondFrameId_MO" type="int" description="Id de la montura" />
				<field name="secondFrameName" type="String" description="Nombre de la montura" />
				<field name="secondFramePrice" type="double" description="Precio de la montura" />
				<field name="secondVariantId_MO" type="String" description="Id de la variante" />
				<field name="secondVariantName" type="String" description="Nombre de la variante" />
				<field name="secondVariantPrice" type="double" description="Precio de la variante" />
				<field name="secondTreatmentOneId_MO" type="int" description="Id del tratamiento" />
				<field name="secondTreatmentOneName" type="String" description="Nombre del tratamiento" />
				<field name="secondTreatmentOnePrice" type="double" description="Precio del tratamiento" />
				<field name="secondTreatmentTwoId_MO" type="int" description="Id del tratamiento" />
				<field name="secondTreatmentTwoName" type="String" description="Nombre del tratamiento" />
				<field name="secondTreatmentTwoPrice" type="double" description="Precio del tratamiento" />
				<field name="secondTreatmentSunId_MO" type="int" description="Id del tratamiento" />
				<field name="secondTreatmentSunName" type="String" description="Nombre del tratamiento" />
				<field name="secondTreatmentSunPrice" type="double" description="Precio del tratamiento" />
				<field name="secondWarrantyId_MO" type="int" description="Id de la garantia" />
				<field name="secondWarrantyName" type="String" description="Nombre de la garantia" />
				<field name="secondWarrantyPrice" type="double" description="Precio de la garantia" />
				<field name="totalPrice" type="double" description="Precio total" />
			</request>
			<response>
				<field name="status" type="String" description="OK, NO_OK" />
				<field name="statusMessage" type="String" description="Descripción del error en caso de error" />
				<field name="orderId_MO" type="int" description="Identificador del pedido guardado" />
			</response>
		</message>
		<!-- ###################################### SAVE ORDER ###################################### -->
		<message name="SendOrder" service="MO" method="sendOrder"
			description="" type="Post">
			<urlPattern>
				<url address="http://${kSendOrderURL}" />
			</urlPattern>
			<request>
				<field name="appId" type="String" description="Id de aplicación" />
				<field name="memberId" type="String" description="Id de socio" />
				<field name="memberSignature" type="String" description="Firma del socio" />
				<field name="orderId_MO" type="int" description="Id del pedido" />
				<field name="name" type="String" description="Nombre del cliente" />
				<field name="email" type="String" description="Email del cliente" />
				<field name="phoneNumber" type="String" description="Telefono del cliente" />
				<field name="comments" type="String" description="Observaciones" />
				<field name="date" type="long" description="Fecha" />
				<field name="promoId_MO" type="int" description="Id de la promocion" />
				<field name="promoName" type="String" description="Nombre de la promocion" />
				<field name="frameId_MO" type="int" description="Id de la montura" />
				<field name="frameName" type="String" description="Nombre de la montura" />
				<field name="framePrice" type="double" description="Precio de la montura" />
				<field name="variantId_MO" type="String" description="Id de la variante" />
				<field name="variantName" type="String" description="Nombre de la variante" />
				<field name="variantPrice" type="double" description="Precio de la variante" />
				<field name="lensTypeId_MO" type="int" description="Id del tipo de lente" />
				<field name="lensTypeName" type="String" description="Nombre del tipo de lente" />
				<field name="lensTypePrice" type="double" description="Precio del tipo de lente" />
				<field name="lensEquipmentId_MO" type="int" description="Id del equipamiento de la lente" />
				<field name="lensEquipmentName" type="String" description="Nombre del equipamiento de la lente" />
				<field name="lensEquipmentPrice" type="double" description="Precio del equipamiento de la lente" />
				<field name="treatmentOneId_MO" type="int" description="Id del tratamiento" />
				<field name="treatmentOneName" type="String" description="Nombre del tratamiento" />
				<field name="treatmentOnePrice" type="double" description="Precio del tratamiento" />
				<field name="treatmentTwoId_MO" type="int" description="Id del tratamiento" />
				<field name="treatmentTwoName" type="String" description="Nombre del tratamiento" />
				<field name="treatmentTwoPrice" type="double" description="Precio del tratamiento" />
				<field name="treatmentSunId_MO" type="int" description="Id del tratamiento" />
				<field name="treatmentSunName" type="String" description="Nombre del tratamiento" />
				<field name="treatmentSunPrice" type="double" description="Precio del tratamiento" />
				<field name="crossSellId_MO" type="int" description="Id de la venta cruzada" />
				<field name="crossSellName" type="String" description="Nombre de la venta cruzada" />
				<field name="crossSellPrice" type="double" description="Precio de la venta cruzada" />
				<field name="crossSellDetailId_MO" type="int" description="Id del detalle de la venta cruzada" />
				<field name="crossSellDetailName" type="String" description="Nombre del detalle de la venta cruzada" />
				<field name="crossSellDetailPrice" type="double" description="Precio del detalle de la venta cruzada" />
				<field name="warrantyId_MO" type="int" description="Id de la garantia" />
				<field name="warrantyName" type="String" description="Nombre de la garantia" />
				<field name="warrantyPrice" type="double" description="Precio de la garantia" />
				<field name="secondFrameId_MO" type="int" description="Id de la montura" />
				<field name="secondFrameName" type="String" description="Nombre de la montura" />
				<field name="secondFramePrice" type="double" description="Precio de la montura" />
				<field name="secondVariantId_MO" type="String" description="Id de la variante" />
				<field name="secondVariantName" type="String" description="Nombre de la variante" />
				<field name="secondVariantPrice" type="double" description="Precio de la variante" />
				<field name="secondTreatmentOneId_MO" type="int" description="Id del tratamiento" />
				<field name="secondTreatmentOneName" type="String" description="Nombre del tratamiento" />
				<field name="secondTreatmentOnePrice" type="double" description="Precio del tratamiento" />
				<field name="secondTreatmentTwoId_MO" type="int" description="Id del tratamiento" />
				<field name="secondTreatmentTwoName" type="String" description="Nombre del tratamiento" />
				<field name="secondTreatmentTwoPrice" type="double" description="Precio del tratamiento" />
				<field name="secondTreatmentSunId_MO" type="int" description="Id del tratamiento" />
				<field name="secondTreatmentSunName" type="String" description="Nombre del tratamiento" />
				<field name="secondTreatmentSunPrice" type="double" description="Precio del tratamiento" />
				<field name="secondWarrantyId_MO" type="int" description="Id de la garantia" />
				<field name="secondWarrantyName" type="String" description="Nombre de la garantia" />
				<field name="secondWarrantyPrice" type="double" description="Precio de la garantia" />
				<field name="totalPrice" type="double" description="Precio total" />
			</request>
			<response>
				<field name="status" type="String" description="OK, NO_OK" />
				<field name="statusMessage" type="String" description="Descripción del error en caso de error" />
			</response>
		</message>
	</messages>
	<types>
		<type name="PromoDTO">
			<field name="promoId" type="int" description="Id de campaña" />
			<field name="imageURL" type="String" description="URL completa de la imagen de campaña" />
			<field name="name" type="String" description="Título de la campaña" />
			<field name="promoDescription" type="String" description="Descripción de la campaña" />
			<field name="lastUpdate" type="double" description="Fecha de la última actualización de la imagen" />
		</type>
		<type name="AgeDTO">
			<field name="ageId" type="String" description="Id de edad" />
			<field name="name" type="String" description="Nombre de la edad" />
			<field name="order" type="int" description="Orden en el que aparecera en los filtros" />
		</type>
		<type name="GenderDTO">
			<field name="genderId" type="String" description="Id de sexo" />
			<field name="name" type="String" description="Nombre del sexo" />
			<field name="order" type="int" description="Orden en el que aparecera en los filtros" />
		</type>
		<type name="FrameTypeDTO">
			<field name="frameTypeId" type="String" description="Id del tipo de montura" />
			<field name="name" type="String" description="Nombre del tipo de montura (Sol, Graduada)" />
			<field name="order" type="int" description="Orden en el que aparecera en los filtros" />
		</type>
		<type name="ColorDTO">
			<field name="colorId" type="String" description="Id de color" />
			<field name="name" type="String" description="Nombre del color" />
			<field name="order" type="int" description="Orden en el que aparecera en los filtros" />
			<field name="webColor" type="String" description="Nombre del color (Web)" />
		</type>
		<type name="CollectionDTO">
			<field name="collectionId" type="String" description="Id de coleccion" />
			<field name="name" type="String" description="Nombre de la coleccion" />
			<field name="order" type="int" description="Orden en el que aparecera en los filtros" />
		</type>
		<type name="MaterialDTO">
			<field name="materialId" type="String" description="Id de material" />
			<field name="name" type="String" description="Nombre del material" />
			<field name="order" type="int" description="Orden en el que aparecera en los filtros" />
		</type>
		<type name="FrameSizeDTO">
			<field name="frameSizeId" type="String" description="Id del calibre" />
			<field name="name" type="String" description="Nombre del calibre" />
			<field name="order" type="int" description="Orden en el que aparecera en los filtros" />
		</type>
		<type name="FramesFilterDTO">
			<field name="frameFilterId" type="int" description="Id de filtro" />
			<field name="defaultFilter" type="boolean" description="Indica si es el filtro por defecto" />
			<field name="ageEnabled" type="boolean" description="Indica si es posible cambiar el filtro de la edad" />			
			<field name="ages" type="int*" description="Listado de Ids de edades posibles" />
			<field name="genderEnabled" type="boolean" description="Indica si es posible cambiar el filtro de la edad" />
			<field name="genders" type="int*" description="Listado de Ids de sexos" />
			<field name="frameTypeEnabled" type="boolean" description="Indica si es posible cambiar el filtro del tipo de montura" />
			<field name="frameTypes" type="int*" description="Listado de Ids de tipos de montura" />
			<field name="colorEnabled" type="boolean" description="Indica si es posible cambiar el filtro del color" />
			<field name="colors" type="int*" description="Listado de Ids de colores" />
			<field name="collectionEnabled" type="boolean" description="Indica si es posible cambiar el filtro de la coleccion" />
			<field name="collections" type="int*" description="Listado de Ids de colecciones" />
			<field name="materialEnabled" type="boolean" description="Indica si es posible cambiar el filtro del material" />
			<field name="materials" type="int*" description="Listado de Ids de materiales" />
			<field name="frameSizeEnabled" type="boolean" description="Indica si es posible cambiar el filtro de los calibres" />
			<field name="frameSizes" type="int*" description="Listado de Ids de calibres" />
		</type>
		<type name="FrameDTO">
			<field name="frameId" type="int" description="id de la montura" />
			<field name="name" type="String" description="Nombre de la montura" />
			<field name="frameDescription" type="String" description="Descripcion de la montura" />
			<field name="order" type="int" description="Orden en el listado" />			
			<field name="frameTypeId" type="String" description="Id del tipo de montura (Sol, Graduada,...)" />
			<field name="genderId" type="String" description="Id del sexo de la montura" />
			<field name="ageId" type="String" description="Id de la edad de la montura" />
			<field name="materialId" type="String" description="Id del material de la montura" />
			<field name="frameSizeId" type="String" description="Id del calibre de la montura" />
			<field name="frameSizeDesc" type="String" description="Descripcion del calibre" />
			<field name="price" type="double" description="Precio de la montura" />
			<field name="collectionId" type="String" description="Id de la coleccion" />
			<field name="isNew" type="Boolean" description="Indica si la montura pertenece a Novedades" />
			<field name="variants" type="VariantDTO*" description="Variantes de la montura" />
		</type>
		<type name="VariantDTO">
			<field name="variantId" type="String" description="Id de la variante" />
      <field name="name" type="String" description="Nombre de la variante" />
			<field name="colorId" type="int" description="Id del color" />
			<field name="order" type="int" description="Orden en el listado" />
			<field name="mainVariant" type="boolean" description="Indicativo si la variante es la principal para esta montura" />
			<field name="imageURL" type="String" description="URL absoluta de la imagen" />
			<field name="lastUpdate" type="double" description="Fecha de la última actualización de la imagen" />
		</type>
		<type name="LensTypeDTO">
			<field name="lensTypeId" type="int" description="id del tipo de lente" />
			<field name="type" type="String" description="Tipo de lente 0 -> monofocal, 1 -> progresivo" />
			<field name="name" type="String" description="Nombre del tipo de lente (monofocal , progresivo)" />
			<field name="lensTypeDescription" type="String" description="Descripción del tipo de lente" />
			<field name="price" type="double" description="Precio del tipo de lente" />
			<field name="imageURL" type="String" description="URL absoluta de la imagen" />
			<field name="lastUpdate" type="double" description="Fecha de la última actualización de la imagen" />
			<field name="lensEquipments" type="LensEquipmentDTO*" description="Listado de equipamientos para el tipo de lente" />
			<field name="treatments" type="TreatmentDTO*" description="Listado de tratamientos para el tipo de lente" />
			<field name="crossSells" type="CrossSellDTO*" description="Listado de ventas cruzadas del tipo de lente" />
		</type>
		<type name="LensEquipmentDTO">
			<field name="lensEquipmentId" type="int" description="Id del equipamiento de la lente" />
			<field name="name" type="String" description="Nombre del equipamiento" />
			<field name="lensEquipmentDescription" type="String" description="Descripción del equipamiento" />
			<field name="price" type="double" description="precio del equipamiento" />
			<field name="imageURL" type="String" description="URL absoluta de la imagen" />
			<field name="lastUpdate" type="double" description="Fecha de la última actualización de la imagen" />
		</type>
		<type name="TreatmentDTO">
			<field name="treatmentId" type="int" description="id del tratamiento" />
			<field name="treatmentType" type="String" description="Tipo de tratamiento (1,2,SOL)" />
			<field name="name" type="String" description="Nombre del tratamiento" />
			<field name="treatmentDescription" type="String" description="Descripción del tratamiento" />			
			<field name="price" type="double" description="precio del tratamiento" />
			<field name="imageURL" type="String" description="URL absoluta de la imagen" />
			<field name="lastUpdate" type="double" description="Fecha de la última actualización de la imagen" />
		</type>
		<type name="CrossSellDTO">
			<field name="crossSellId" type="int" description="Id de venta cruzada" />
			<field name="name" type="String" description="Nombre de la venta cruzada" />
			<field name="crossSellDescription" type="String" description="Descripción de la venta cruzada" />			
			<field name="price" type="double" description="Coste adicional de la venta cruzada" />
			<field name="priceType" type="String" description="Indica si el precio es absoluto o porcentaje" />
			<field name="framesFilterId" type="int" description="Id del filtro a aplicar para la segunda montura" />
			<field name="optionalInheritance" type="Boolean" description="Indica si la venta cruzada permite heredar los tratamientos a la segunda gafa" />			
			<field name="requiredInheritanceTreatment1" type="Boolean" description="Indica si la herencia del tratamiento 1 es obligatoria" />
			<field name="requiredInheritanceTreatment2" type="Boolean" description="Indica si la herencia del tratamiento 2 es obligatoria" />
			<field name="requiredInheritanceTreatmentSun" type="Boolean" description="Indica si la herencia del tratamiento Sol es obligatoria" />
			<field name="hasSecondFrame" type="Boolean" description="Indica si la venta cruzada incluye una segunda montura" />
			<field name="secondFrameDiscount" type="double" description="Si existe indica que al precio de la segunda gafa se le aplica el descuento" />
