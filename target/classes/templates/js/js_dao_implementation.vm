function ${daoClassName}() {

	this.create = new function(json) {
	
		var returnValue = new ${className}();

#foreach ($field in $baseFields)
		returnValue.${field.name} = json.${field.name};
#end

#foreach ($field in $baseArrayFields)
		returnValue.${field.name} = json.${field.name};
#end

#foreach ($field in $objectFields)
		var obj${field.name}DAO = new ${field.typeJavascriptDAO}();
		returnValue.${field.name} = obj${field.name}DAO.create(json.${field.name});	
#end

#foreach ($field in $objectArrayFields)
		var ${field.name}Values = new Array();
		for (var i=0; i<json.${field.name}.length; i++) {
			var objDAO = new ${field.typeJavascriptDAO}();
			${field.name}Values[i] = objDAO.create(json.${field.name});
		}
		returnValue.${field.name} = ${field.name}Values;
#end
	
		return returnValue;
	}
}
